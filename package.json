{
  "name": "wp-post-notifications",
  "version": "0.0.3",
  "description": "Let users subscribe to post updates",
  "main": "post-notifications.js",
  "scripts": {
    "start": "npm run watch & npm run start-server",
    "start-server": "./node_modules/http-dev-server/bin/http-dev-server -f httpconfig.js",
    "start-parcel": "./node_modules/parcel-bundler/bin/cli.js index.html",
    "watch": "./node_modules/parcel-bundler/bin/cli.js watch ./post-notifications.js",
    "build": "./node_modules/parcel-bundler/bin/cli.js build ./post-notifications.js",
    "lint": "./node_modules/.bin/eslint ."
  },
  "keywords": [
    "wordpress",
    "ajax"
  ],
  "author": "Rafael Nowrotek",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.16.0",
    "http-dev-server": "^0.2.3",
    "http-server": "^0.11.1",
    "parcel-bundler": "^1.5.0"
  },
  "dependencies": {
    "remoteform": "^0.0.3",
    "whatwg-fetch": "^2.0.4"
  }
}
